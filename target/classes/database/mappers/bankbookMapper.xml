<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.cw.s1.bankbook.BankBookDAO">
 	<delete id="setDelete" parameterType="java.lang.Long">
 		DELETE BANKBOOK WHERE BOOKNUM=#{bookNum} <!-- DAO long타입 변수명 -->
 	</delete>
 	
 	<insert id="setInsert" parameterType="com.cw.s1.bankbook.BankBookDTO" >
 		INSERT INTO BANKBOOK (BOOKNUM, BOOKNAME, BOOKRATE, BOOKSALE) 
 		VALUES (BANKBOOK_SEQ.NEXTVAL, #{bookName}, #{bookRate}, #{bookSale})
 	</insert>
  	
  	<select id="getList" resultType="com.cw.s1.bankbook.BankBookDTO">
  		SELECT * FROM BANKBOOK
  	</select>
  	
  	<select id="getSelect" parameterType="com.cw.s1.bankbook.BankBookDTO" resultType="com.cw.s1.bankbook.BankBookDTO">
  		SELECT * FROM BANKBOOK WHERE BOOKNUM=#{bookNum}
  	</select>
	
	
  </mapper>